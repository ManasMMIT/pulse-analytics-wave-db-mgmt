      <mj-text mj-class='organization-header-text'>
        Influencer
      </mj-text>
      <mj-text align='center' padding='2px 0' mj-class='view-account-link'>
        <a
          href='https://pulse-tools.com/pathways/accounts/pathways/{{ orgSlugs[orgName] }}/influencers'
          style='color: #0668D9;'
